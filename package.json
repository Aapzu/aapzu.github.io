{
  "private": true,
  "name": "haanpuu.fi",
  "description": "The Portfolio of Aapeli Haanpuu",
  "repository": "https://github.com/aapzu/portfolio-page",
  "author": "Aapeli Haanpuu",
  "homepage": "https://haanpuu.fi",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint": "npm-run-all --parallel lint:*",
    "start": "phenomic start",
    "build": "phenomic build",
    "pretest": "npm run lint",
    "test": "npm run build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "phenomic": {
    "CNAME": false
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      },
      "development": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "babel-preset-react-optimize",
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      },
      "webpack-development": {
        "presets": [
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "webpack-production": {
        "presets": [
          "babel-preset-react-optimize",
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": "./node_modules/phenomic/lib/eslint-config-recommended/index.js"
  },
  "stylelint": {
    "extends": "./node_modules/phenomic/lib/stylelint-config-recommended/index.js"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "axios": "^0.15.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx-self": "^6.11.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bootstrap": "^4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "css-mqpacker": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^6.4.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "history": "^2.0.0",
    "json-loader": "^0.5.2",
    "npm-run-all": "^1.7.0",
    "phenomic": "^0.20.4",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-color-function": "^3.0.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-font-magician": "^1.6.1",
    "postcss-loader": "^0.13.0",
    "postcss-nested": "^1.0.0",
    "postcss-remove-unused": "^1.0.1",
    "postcss-reporter": "^1.4.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "rc-scroll-anim": "^0.6.5",
    "react": "^15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.4.1",
    "react-helmet": "^3.0.0",
    "react-hot-loader": "^3.1.3",
    "react-masonry-component": "^5.0.7",
    "react-redux": "^4.4.8",
    "react-router": "^2.3.0",
    "react-scrollchor": "^2.2.0",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "react-visibility-sensor": "^3.7.0",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "roboto-fontface": "^0.7.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.13.0",
    "url-loader": "^0.5.8",
    "warning": "^3.0.0",
    "webpack": "^2.7.0",
    "whatwg-fetch": "^0.11.0",
    "snyk": "^1.114.0"
  },
  "snyk": true
}
